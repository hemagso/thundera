        {"args": { "domain_type": "range", "domain_value": "[-5, +inf)"}, "expected": "0000011111111110"},
        {"args": { "domain_type": "range", "domain_value": "(-5, +inf)"}, "expected": "0000001111111110"},
        {"args": { "domain_type": "single", "domain_value": -100000000     }, "expected": "1000000000000000"},
