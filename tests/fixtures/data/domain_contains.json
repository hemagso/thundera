        {"args": { "domain_type": "single", "domain_value": 6              }, "expected": "0000000000010000"},
        {"args": { "domain_type": "single", "domain_value": 100            }, "expected": "0000000000001000"},
        {"args": { "domain_type": "single", "domain_value": 100000         }, "expected": "0000000000000100"},
        {"args": { "domain_type": "single", "domain_value": 100000000      }, "expected": "0000000000000010"},
